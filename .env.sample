
# DiaBuddy Environment Variables

# Note: All Vite environment variables must be prefixed with VITE_

# Gemini API Key for AI features
VITE_GEMINI_API_KEY=

# OpenAI API Key for LangGraph/LangChain integration
VITE_OPENAI_API_KEY=

# Enable LangGraph features (set to 'true' to enable)
VITE_USE_LANGGRAPH=false

# Backend service URL for LangGraph agent
VITE_BACKEND_URL=http://localhost:3001

# Google Calendar API Key (optional)
VITE_GOOGLE_API_KEY=

# Other API keys
# VITE_OTHER_API_KEY=
